{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "A boilerplate starter for server-side rendered React applications built with Razzle.",
  "scripts": {
    "clean": "rm -rf node_modules && rm package-lock.json",
    "dev": "razzle start",
    "test": "razzle test --env=jsdom",
    "build": "razzle build",
    "start": "NODE_ENV=production node build/server.js",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diogocapela/react-starter.git"
  },
  "author": "Diogo Capela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diogocapela/react-starter/issues"
  },
  "homepage": "https://github.com/diogocapela/react-starter#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "html-minifier": "^3.5.21",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "razzle": "^2.4.1",
    "razzle-heroku": "^3.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.6.1"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0"
  }
}
